@using BusinessObject
@{
    ViewData["Title"] = "Home Page";

    List<EmployeeBO> listado = (List<EmployeeBO>)ViewBag.Listado;

}

<table class="table table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>NOMBRE</th>
            <th>SALARIO</th>
            <th>EDAD</th>
            <th>FOTO PERFIL</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in listado)
        {
            <tr>
                <td>@item._id</td>
                <td>@item.nombre</td>
                <td>@item.salario</td>
                <td>@item.edad</td>
                <td>
                    <img src="@item.perfil" height="30" width="30" />

                </td>
                <td></td>
            </tr>

        }

    </tbody>
</table>